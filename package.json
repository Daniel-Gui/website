{
	"name": "website",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "svelte-kit sync && node scripts/prebuild.mjs && vite build",
		"preview": "wrangler dev .svelte-kit/cloudflare/_worker.js --port 4173",
		"prepare": "npm run types && svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"types": "wrangler types"
	},
	"devDependencies": {
		"@eslint/compat": "^1.4.0",
		"@eslint/js": "^9.39.1",
		"@sveltejs/adapter-cloudflare": "^7.2.4",
		"@sveltejs/enhanced-img": "^0.10.0",
		"@sveltejs/kit": "^2.49.1",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/typography": "^0.5.19",
		"@tailwindcss/vite": "^4.1.17",
		"@types/node": "^22",
		"eslint": "^9.39.1",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.13.1",
		"globals": "^16.5.0",
		"mdsvex": "^0.12.6",
		"prettier": "^3.7.4",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.7.2",
		"svelte": "^5.45.6",
		"svelte-check": "^4.3.4",
		"tailwindcss": "^4.1.17",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.48.1",
		"vite": "^7.2.6",
		"wrangler": "^4.56.0"
	},
	"dependencies": {
		"@fontsource-variable/archivo": "^5.2.8",
		"@fontsource-variable/funnel-display": "^5.2.8",
		"@fontsource-variable/jetbrains-mono": "^5.2.8",
		"clsx": "^2.1.1",
		"lenis": "^1.3.17",
		"mode-watcher": "^1.1.0",
		"motion": "^12.26.1",
		"runed": "^0.37.1",
		"shiki": "^3.21.0",
		"tailwind-merge": "^3.4.0"
	}
}
